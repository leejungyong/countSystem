/* pages/index/home.wxss */
page{
  background: #F5F5F5;
}
.out-wrap{
  padding-left: 40rpx;
  padding-top: 20rpx;
  background: #fff;
}
.item{
  /* margin-top: 30rpx; */
  display: flex;
  align-items: center;
  font-size: 32rpx;
  border-bottom: 2rpx solid #e5e5e5;
  padding: 22rpx 0 20rpx 0;
}
.item .text{
  width: 170rpx;
}
.item .input{
  flex: 1;
  /* padding-left: 20rpx; */
  box-sizing: border-box;
  color: #666;
  
}

.pl{
  color: #999;
}
.active{
  outline: 0;
  border: 1px solid #d8a163 !important;
  box-shadow: 0px 0px 10px 0px #d8a163;
}
.star::after{
  content: "*";
  color: red;
}
.bottom {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 180rpx;
  display: flex;
  justify-content: center;
  font-size: 36rpx;
}
.btn{
  background: #d8a163;
  color: #fff;
  text-align: center;
  width: 670rpx;
  height:100rpx;
  line-height: 100rpx;
  border-radius: 10rpx;
}
