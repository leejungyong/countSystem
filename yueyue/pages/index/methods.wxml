<view class="title">
  数据挖掘方法(非必选)
</view>
<view class="list">
  <view class="item" wx:for="{{list}}" wx:key="index" id="{{index}}" bindtap="select">
    <view class="name">{{item.name}}</view>
    <view class="check">
      <view class="minibox {{item.checked?'checked':''}}"></view>
    </view>
  </view>
</view>
 <view class="bottom">
        <view class="btn" bindtap="next">下一步</view>
    </view>